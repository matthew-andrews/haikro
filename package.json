    "test": "npm run jshint && npm run lintspaces && mocha test/*.test.js"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "denodeify": "^1.1.2",
    "glob": "^5.0.5",
    "isomorphic-fetch": "^1.0.0",
    "semver": "^4.3.3",
    "shellpromise": "^1.1.0"
  },
  "devDependencies": {
    "jshint": "^2.5.10",
    "lintspaces-cli": "^0.1.0",
    "mocha": "^2.0.1"
  }
}

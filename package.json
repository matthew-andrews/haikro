{
  "name": "haikro",
  "version": "2.3.1",
  "main": "main.js",
  "bin": {
    "haikro": "bin/haikro.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthew-andrews/haikro.git"
  },
  "scripts": {
    "jshint": "jshint `npm run -s js-files`",
    "lintspaces": "lintspaces -i js-comments -e .editorconfig `npm run -s js-files`",
